<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
      <h1 class="text-center"> All cocktails</h1>
      <% @cocktails.each do |cocktail| %>
        <ul>
          <li>
            <%= link_to cocktail.name, cocktail_path(cocktail) %>
          </li>
        </ul>
      <% end %>
    </div>
  </div>
</div>


